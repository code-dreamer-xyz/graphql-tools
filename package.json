{
  "name": "apollo-proxy",
  "version": "0.1.0",
  "description": "A GraphQL proxy",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "nodemon test/server-index.js",
    "test": "mocha --reporter spec --full-trace test/index.js",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apollostack/apollo-proxy.git"
  },
  "keywords": [
    "GraphQL",
    "Apollo",
    "Meteor",
    "Proxy",
    "Cache",
    "Javascript"
  ],
  "author": "Jonas Helfer <helfer@meteor.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/apollostack/apollo-proxy/issues"
  },
  "homepage": "https://github.com/apollostack/apollo-proxy#readme",
  "dependencies": {
    "babel-polyfill": "^6.5.0",
    "fs": "0.0.2",
    "graphql": "^0.4.18",
    "graphql-shorthand-parser": "git+ssh://git@github.com:apollostack/graphql-shorthand-parser.git#include-dist"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "6.3.21",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-loader": "6.2.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "dataloader": "^1.1.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-import": "^1.1.0",
    "express": "^4.13.4",
    "express-graphql": "^0.4.13",
    "mocha": "^2.3.3",
    "nodemon": "^1.9.1",
    "request-promise": "^2.0.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base",
      "plugin:import/errors"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "no-console": 0
    },
    "env": {
      "mocha": true
    }
  }
}
